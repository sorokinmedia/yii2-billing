{
  "name": "sorokinmedia/yii2-billing",
  "description": "Billing component for Yii2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "yii 2",
    "billing"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ruslan Gilyazetdinov",
      "email": "ma3obblu@gmail.com",
      "homepage": "https://vk.com/ma3obblu"
    }
  ],
  "support": {
    "issues": "https://github.com/sorokinmedia/yii2-billing/issues?state=open",
    "source": "https://github.com/sorokinmedia/yii2-billing"
  },
  "require": {
    "yiisoft/yii2-gii": "~2.0",
    "sorokinmedia/yii2-ar-relations": "dev-master",
    "sorokinmedia/yii2-helpers": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "sorokinmedia\\billing\\": "src/",
      "sorokinmedia\\billing\\tests\\": "tests/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
